<div class="panel panel-primary">
  <div class="panel-heading">
    All Contacts
  </div>
  <br/>
  <div class="panel-body">
    <table class="table">
      <thead>
      <th>ID</th>
      <th>First name</th>
      <th>Last Name</th>
      <th>Company Name</th>
      </thead>
      <tbody>
      <%- @contacts.each do |contact| %>
          <tr>
            <td><%= contact.id %></td>
            <td><%= contact.first_name %></td>
            <td><%= contact.last_name %></td>
            <td><%= contact.company_name %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>

</div>